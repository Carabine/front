  isModalOpen: false,
  mediafiles: [],
  files: [],
  isUploading: false,
  error: '',
  videoPercent: 0,
  currentVideo: null
};
const mediafilesReducer = (state = initialState, action) => {
  switch (action.type) {
    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_IS_MODAL_OPEN:
      {
        return { ...state,
          isModalOpen: action.isModalOpen
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].ADD_MEDIAFILES:
      {
        console.log(action.mediafiles);
        return { ...state,
          mediafiles: [...action.mediafiles, ...state.mediafiles]
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_MEDIAFILES:
      {
        return { ...state,
          mediafiles: action.mediafiles
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].REMOVE_MEDIAFILE:
      {
        return { ...state,
          mediafiles: state.mediafiles.filter(m => m.id !== action.mediafileId)
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].ADD_FILES:
      {
        return { ...state,
          files: [...state.files, ...action.files]
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].REMOVE_FILE:
      {
        if (!action.fileId) {
          return { ...state,
            files: state.files.slice(0, -1)
          };
        }

        return { ...state,
          files: state.files.filter(f => f.id !== action.fileId)
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].CLEAR_FILES:
      {
        return { ...state,
          files: []
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_IS_UPLOADING:
      {
        return { ...state,
          isUploading: action.isUploading
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_ERROR:
      {
        return { ...state,
          error: action.error
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_FILE_PERCENT:
      {
        return { ...state,
          filePercent: action.percent
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].CHANGE_VIDEO_NAME:
      {
        return { ...state,
          mediafiles: state.mediafiles.map(m => m.id === action.videoId ? { ...m,
            name: action.name
          } : m)
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_CURRENT_VIDEO:
      {
        return { ...state,
          currentVideo: action.video
        };
      }

    case _actions_mediafiles__WEBPACK_IMPORTED_MODULE_0__["MediafilesActions"].SET_VIDEO_EDIT_MODE:
      {
        return { ...state,
          mediafiles: state.mediafiles.map(m => m.id === action.videoId ? { ...m,
            editMode: action.editMode
          } : m)
        };
      }

    default:
      return state;
  }
