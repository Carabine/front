  pageTitleShadow: false,
  pageTitleBreadcrumb: false,
  pageTitleIconBox: true,
  pageTitleDescription: true
};
const themeOptionsReducer = (state = initialState, action) => {
  switch (action.type) {
    // Sidebar
    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_SIDEBAR_SHADOW:
      return { ...state,
        sidebarShadow: action.sidebarShadow
      };

    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_SIDEBAR_FIXED:
