const AuthActions = {
  SUCCESS_AUTH: 'auth/SUCCESS-AUTH',
  FAIL_AUTH: 'auth/FAIL-AUTH',
  CHANGE_LANG: 'auth/CHANGE_LANG'
};
const successAuth = () => ({
  type: AuthActions.SUCCESS_AUTH
});
const failAuth = () => ({
  type: AuthActions.FAIL_AUTH
});
const initializeSuccess = () => ({
  type: AuthActions.INITIALIZE_SUCCESS
});
const changeLang = lang => ({
  type: AuthActions.CHANGE_LANG,
  lang
});
const auth = () => {
  return async dispatch => {
    try {
      const res = await _api__WEBPACK_IMPORTED_MODULE_0__["authAPI"].auth();

      if (res.data && res.data.lang) {
        dispatch(changeLang(res.data.lang));
      }

      dispatch(successAuth());
    } catch (err) {
      dispatch(failAuth());
    }
  };
};
const logout = () => {
  return async dispatch => {
    try {
      await _api__WEBPACK_IMPORTED_MODULE_0__["authAPI"].logout();
      dispatch(failAuth());
    } catch (err) {
      console.log(err);
    }
  };
};

