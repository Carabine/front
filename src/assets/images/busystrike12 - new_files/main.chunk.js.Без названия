        sidebarToggle: action.sidebarToggle
      };

    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_SIDEBAR_HOVER:
      return { ...state,
        sidebarHover: action.sidebarHover
      };

    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_SIDEBAR_USERBOX:
      return { ...state,
        sidebarUserbox: action.sidebarUserbox
      };
    // Header

    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_HEADER_FIXED:
      return { ...state,
        headerFixed: action.headerFixed
      };

    case _actions_themeOptions__WEBPACK_IMPORTED_MODULE_0__["ThemeOptionsActions"].SET_HEADER_SHADOW:
      return { ...state,
