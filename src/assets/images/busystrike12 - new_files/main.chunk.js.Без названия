  return async dispatch => {
    try {
      const id = Object(_utils_uuid__WEBPACK_IMPORTED_MODULE_2__["uuid"])();
      const formData = new FormData();

      for (let i = 0; files.length > i; i++) {
        console.log(files[i]);
        formData.append(i, files[i]);
      }

      dispatch(setIsUploading(true));
      const {
        data: {
          domain
        }
      } = await _api_index__WEBPACK_IMPORTED_MODULE_0__["mediafilesAPI"].getMediaserver();
      const eventSource = new EventSource(`${_config__WEBPACK_IMPORTED_MODULE_1__["default"].protocol}://${domain}/sse/${id}`, {
        withCredentials: true
      });
      eventSource.addEventListener('videoHasUploaded:' + id, async event => {
        const data = JSON.parse(event.data);
        const res = await _api_index__WEBPACK_IMPORTED_MODULE_0__["mediafilesAPI"].saveMediafile({ ...data,
          domain
        });
        dispatch(addMediafiles([res.data]));
        dispatch(removeFile());
        dispatch(setFilePercent(0));
      });
      eventSource.addEventListener('progressVideoData:' + id, event => {
        const data = JSON.parse(event.data);
        dispatch(setFilePercent(data.percent));
      });
      await axios__WEBPACK_IMPORTED_MODULE_3___default.a.post(`${_config__WEBPACK_IMPORTED_MODULE_1__["default"].protocol}://${domain}/mediafiles/${id}`, formData, {
        withCredentials: true,
        headers: {
          'content-type': 'multipart/form-data'
        }
      });
      dispatch(clearFiles());
      dispatch(setIsModalOpen(false));
      dispatch(setFilePercent(0));
      setTimeout(() => eventSource.close(), 10000);
    } catch (err) {
      dispatch(setError('uploadingError'));
    }

    dispatch(setIsUploading(false));
  };
};
const fetchMediafiles = () => {
  return async dispatch => {
    try {
      const res = await _api_index__WEBPACK_IMPORTED_MODULE_0__["mediafilesAPI"].fetchMediafiles();
      dispatch(setMediafiles(res.data));

      if (res.data.length) {
        dispatch(setCurrentVideo(res.data[0]));
      }
    } catch (err) {
      dispatch(setError('fetchingMediafilesError'));
    }
  };
};
const deleteMediafile = mediafileId => {
  return async dispatch => {
    try {
      await _api_index__WEBPACK_IMPORTED_MODULE_0__["mediafilesAPI"].deleteMediafile(mediafileId);
      dispatch(removeMediafile(mediafileId));
    } catch (err) {
      dispatch(setError('deletingMediafilesError'));
    }
  };
};

